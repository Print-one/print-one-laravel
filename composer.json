{
    "name": "print-one/print-one-laravel",
    "description": "This is my package print-one",
    "keywords": [
        "nexibi",
        "laravel",
        "print.one",
        "printing",
        "rest",
        "print-and-mail",
        "postcard",
        "post",
        "api"
    ],
    "homepage": "https://github.com/Print-one/print-one-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Nexxtmove",
            "email": "tech@nexxtmove.nl",
            "role": "Developer"
        },
        {
            "name": "Sibi",
            "email": "dev@sibi.nl",
            "role": "Developer"
        },
        {
            "name": "Print.one Development",
            "email": "dev@print.one",
            "role": "Developer"    
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/contracts": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.12.0|^1.13.6"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Nexibi\\PrintOne\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexibi\\PrintOne\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nexibi\\PrintOne\\PrintOneServiceProvider"
            ],
            "aliases": {
                "PrintOne": "Nexibi\\PrintOne\\Facades\\PrintOne"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
